<h1>Usuarios Registrados</h1>
<table class="table table-hovered table-bordered">
    <thead>
    <tr>
        <th>Id</th>
        <th>Email</th>
        <th>Acciones</th>
    </tr>        
    </thead>
    <tbody>
    <% @users.each do |user| %>    
    <tr>
        <td><%= user.id %></td>
        <td><%= user.email %></td>
        <td><a href="/users/<%= user.id %>"> Ver </a> </td>
    </tr>
    <% end %>
    </tbody>
    
</table>